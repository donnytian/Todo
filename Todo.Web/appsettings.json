{
    "ConnectionStrings": {
        "Default": "Server=(LocalDb)\\MSSQLLocalDB;Database=todo;Initial Catalog=todo;Integrated Security=True",
        "Mongo": "mongodb://localhost:27017/todo"
    },
    "Logging": {
        "IncludeScopes": false,
        "Debug": {
            "LogLevel": {
                "Default": "Warning"
            }
        },
        "Console": {
            "LogLevel": {
                "Default": "Warning"
            }
        }
    },
    "JwtSecurityToken": {
        "SigningKey": "YouCannotAlterTokenIfYouCannotHoldThisVeryLongKey",
        "SigningAlgorithm": "HS256",
        "Issuer": "http://todo.com",
        "Audience": "http://todo.com",
        "ExpireInMinutes": 60,
        "RememberMeExpireInDays": 14,
        "UseCookie": false,
        "CookieName": "access_token"
    }
}
